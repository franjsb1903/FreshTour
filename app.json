    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptative_fresh_tour.png",
        "backgroundColor": "#219653"
      },
      "package": "com.franjsb.FreshTour"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
