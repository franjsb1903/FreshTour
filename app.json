{
  "expo": {
    "name": "FreshTour",
    "slug": "FreshTour",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_fresh_tour.png",
    "splash": {
      "image": "./assets/fresh_tour_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
